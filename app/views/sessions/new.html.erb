<%= form_for :camp_counselor, url: signin_path, method: :post do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.submit "Login" %>
<% end %>
or
<%= form_tag new_camp_counselor_path, method: :get do %>
<%= submit_tag "Create Camp Counselor" %>
<% end %>
