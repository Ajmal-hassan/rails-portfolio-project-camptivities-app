<h2>Welcome to Camptivities!</h2>
<br>
<p>
  <%= form_for :camp_counselor, autocomplete: "off", url: signin_path, method: :post do |f| %>
  <%= f.label :name %>
  <br>
  <%= f.text_field :name %>
  <br>
  <%= f.label :password %>
  <br>
  <%= f.password_field :password %>
  <br>
  <%= f.submit "Login" %>
  <% end %>
  <br>
  or
  <br>
  <%= form_tag new_camp_counselor_path, method: :get do %>
  <%= submit_tag "Create Account" %>
  <% end %>
</p>
