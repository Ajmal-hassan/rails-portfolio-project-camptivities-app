<h1>Create Your Camp Counselor Profile:</h1>
<br>
<%= form_for @counselor do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <br>
  <%= f.radio_button(:age, true) %>
  <%= f.label(:leader, "I'm 18 or older") %>
  <%= f.radio_button(:age, false) %>
  <%= f.label(:leader, "I'm under 18") %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>
  <%= f.submit "Create Counselor" %>
<% end %>
