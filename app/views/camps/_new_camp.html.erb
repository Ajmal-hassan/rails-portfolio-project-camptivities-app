<%= form_for camp do |f| %>
  <%= f.label :name, "Camp Name" %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :location, "Location" %>
  <%= f.text_field :location %>
  <br>
  <%= f.label :description, "Description" %>
  <%= f.text_field :description, size: "50x10" %>
  <br>
  <br>
  <%= f.submit "Add Camp" %>
<% end %>
